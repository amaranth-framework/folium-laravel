{
    "name": "itmcdev/folium",
    "description": "PHP Wrapper for RESTFul applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragos Cirjan",
            "email": "dragos.cirjan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "symfony/http-foundation": "^3.1",
        "illuminate/database": "^5.8@dev",
        "phpunit/phpunit": "^7.5@dev",
        "phpunit/dbunit": "^4.0@dev"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Folium\\": "Folium/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "local-tests": "docker build -t implementation-tests . && docker run --rm -it -v $(pwd):/code implementation-tests sh -c \"cd /code && phpunit \""
    }
}
